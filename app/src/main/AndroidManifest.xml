<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pos.yza.yzapos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".createtransaction.NewTransactionActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".createtransaction.CustomerDetailsActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".createtransaction.PaymentActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".createtransaction.ProductCategoryActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".createtransaction.ProductActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".adminlogin.AdminLoginActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".adminoptions.AdminOptionsActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ViewTransactionActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".ViewProductsActivity" />
        <activity android:name=".manageproduct.ManageProductActivity"></activity>
    </application>

</manifest>